@using BackendMVC1.Models;
@model List<Car>
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>


<ul>
    @foreach (Car cars in Model)
    {
        <a asp-controller="Model1" asp-action="Detail" asp-route-id="@cars.Id">
        <li>@cars.Count</li>
        </a>
    }
</ul>